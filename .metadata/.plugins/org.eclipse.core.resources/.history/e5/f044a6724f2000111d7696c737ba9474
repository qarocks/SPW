import java.util.NoSuchElementException;
import java.util.concurrent.TimeUnit;
import java.util.regex.Pattern;

public class InitialSanity_IE  {
	
	WebDriver driver;
	@Before
	public void setUp() throws Exception {
		
		driver=new InternetExplorerDriver();
		//driver.manage().timeouts().implicitlyWait(5000, TimeUnit.SECONDS);
		//selenium = new DefaultSelenium("localhost", 4444, "*iexplore", "http://192.168.1.129/");
		//selenium.start();
	}

	@Test
	public void InitialSanityMain() throws Exception {
		
		driver.get("http://192.168.1.129");
		//Functions.MyWaitfunc_id(driver,"id_username");
		driver.findElement(By.id(("id_username"))).sendKeys("admin");
		//driver.findElement(By.cssSelector("input[id='id_username']")).sendKeys("admin");
		//selenium.open("/");
	    
		//selenium.type("id=id_username", "admin");
		
	}
	private boolean isElementPresent(By by) {
		try {
			driver.findElement(by);
			return true;
		} catch (NoSuchElementException e) {
			return false;
		}
	}
	@After
	public void tearDown() throws Exception {
		//selenium.stop();
		driver.quit();
	}
}
